<?xml version="1.0" encoding="UTF-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true" >
        <shape>
            <solid
                android:color="@color/symptom_color_selected" />
            <stroke
                android:width="0dp"
                android:color="#222222" />
            <corners
                android:radius="1dp" />

        </shape>
    </item>

    <item>
        <shape>
            <gradient
                android:startColor="@color/colorPrimaryDark"

                android:endColor="@color/lite_dark_blue"
                android:angle="270" />
            <stroke
                android:width="0dp"
                android:color="#000" />
            <corners
                android:radius="1dp" />
        </shape>
    </item>
    </selector>